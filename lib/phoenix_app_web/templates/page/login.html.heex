<body>
  <!--banner-->
  <section id="banner">
    <div class="bg-color">
      <header id="header">
        <div class="container">
          <div id="mySidenav" class="sidenav">
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
            <a href="/">Catálogo</a>
            <a href="/login">Login</a>
            <a href="/register">Criar Conta</a>

          </div>
          <!-- Use any element to open the sidenav -->
          <span onclick="openNav()" class="pull-right menu-icon">☰</span>
        </div>
      </header>
      <div class="container">
        <div class="row">
          <div class="inner text-center">
            <h1 class="logo-name text-center">Login</h1>

            <form class="login-form">
                <!-- Email input -->
                <div class="form-outline mb-4">
                    <input type="email" name="email-input" id="user-email-input" class="form-control" placeholder="Email" value />
                </div>
                
                <!-- Password input -->
                <div class="form-outline mb-4">
                    <input type="password" name="password-input" id="user-password-input" class="form-control" placeholder="Password" value />
                </div>
                
                <!-- 2 column grid layout for inline styling -->
                <div class="row mb-4">
                    <div class="col d-flex justify-content-center">
                    <!-- Checkbox -->
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" value="" id="form2Example31" checked />
                        <label class="form-check-label" for="form2Example31"> Lembrar de mim </label>
                    </div>
                    </div>
                
                    <div class="col">
                    <!-- Simple link -->
                    <a href="#!">Esqueceu a Senha?</a>
                    </div>
                </div>
                
                <!-- Submit button -->
                <button type="button" class="btn btn-primary btn-block mb-4" onclick="login()">Entrar</button>
                
                <!-- Register buttons -->
                <div class="text-center">
                    <p>Não possui conta? <a href="/register">Registre-se</a></p>
                </div>
                </form>

            
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- / banner -->
  

  <!-- footer -->
  <footer class="footer text-center">
    <div class="footer-top">
      <div class="row">
        <div class="col-md-offset-3 col-md-auto text-center">
          <div class="widget">
            <h4 class="widget-title">Delicious</h4>
            <address>324 Ellte Road<br>Delhi, DL 110013</address>
            <div class="social-list">
              <a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a>
              <a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a>
            </div>
            <p class="copyright clear-float">
              © Delicious Theme. All Rights Reserved
